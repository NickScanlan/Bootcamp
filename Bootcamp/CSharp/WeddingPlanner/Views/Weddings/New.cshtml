@model Wedding
<a href="weddings">Home</a>
<div class="container w-75 mx-auto">
  <h2 class="text-center">New Wedding</h2>

  <form
    class="shadow p-3 rounded"
    asp-controller="Weddings"
    asp-action="Create"
    method="POST"
  >
    <div class="mb-3">
      <label asp-for="WedderOne"></label>
      <span asp-validation-for="WedderOne" class="text-danger font-weight-bold">
      </span>
      <input class="form-control" asp-for="WedderOne" />
    </div>

    <div class="mb-3">
      <label asp-for="WedderTwo"></label>
      <span asp-validation-for="WedderTwo" class="text-danger font-weight-bold">
      </span>
      <input class="form-control" asp-for="WedderTwo" />
    </div>

    <div class="mb-3">
      <label asp-for="WeddingDate"></label>
      <input class="form-control" asp-for="WeddingDate" />
    </div>
    
    
    <div class="mb-3">
      <label asp-for="Address"></label>
      <input class="form-control" asp-for="Address" />
    </div>

    <button class="btn btn-success">Submit</button>
  </form>
</div>